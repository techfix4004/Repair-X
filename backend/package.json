{
  "name": "repairx-backend",
  "version": "1.0.0",
  "description": "RepairX Backend API - Production-ready repair service platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:tsc": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "echo 'Running tests without strict build' && jest --testPathPattern='auth|devices'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --max-warnings 20",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts",
    "quality:check": "npm run lint && npm test && echo 'Build check completed'"
  },
  "keywords": [
    "repair",
    "service",
    "platform",
    "api",
    "fastify",
    "typescript"
  ],
  "author": "RepairX Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@opentelemetry/exporter-prometheus": "^0.203.0",
    "@opentelemetry/instrumentation-fastify": "^0.48.0",
    "@opentelemetry/instrumentation-http": "^0.203.0",
    "@opentelemetry/instrumentation-pg": "^0.55.0",
    "@opentelemetry/instrumentation-redis": "^0.51.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@prisma/client": "^6.13.0",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/ws": "^8.18.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "decision-tree": "^0.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "ml-knn": "^3.0.0",
    "natural": "^8.1.0",
    "node-2fa": "^2.0.3",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "simple-statistics": "^7.8.8",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.18.3",
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-jest": "^29.0.1",
    "fastify": "^5.5.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prisma": "^6.13.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
